var flow = When($feature.prod_list == 'zenivex', 13,
                $feature.prod_list == 'anvil', 11,
                $feature.prod_list == 'duet', 7,
                $feature.prod_list == 'fyfanon', 0,
                0
                );

var rate = IIf($feature.rate_list == 'full', 1, 0.5);
var startTime = $feature.time_start;
var endTime = $feature.time_end;
var elapsedTime = When(DateDiff(endTime, startTime, 'minutes') == 0,1,
    DateDiff(endTime, startTime, 'minutes')
    );

return elapsedTime * flow * rate
